#include <iostream>
#include <stack>
#include <string>
#include <cctype>
using namespace std;
int main(){
  string s;
  getline(cin,s);
  stack<int> st;
  for(char c: s){
  if(isdigit(static_cast<unsigned char>(c))) st.push(c-'0'); else if(!isspace(static_cast<unsigned char>(c))){ int b=st.top(); st.pop(); int a=st.top(); st.pop(); if(c=='+') st.push(a+b); else if(c=='-') st.push(a-b); else if(c=='*') st.push(a*b); else if(c=='/') st.push(a/b); } } cout<<st.top(); return 0; }
