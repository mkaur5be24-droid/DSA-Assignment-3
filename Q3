#include <iostream>
#include <stack>
#include <string>
using namespace std;
int main(){
  string s;
  getline(cin,s);
  stack<char> st;
  for(char c:s){
    if(c=='('||c=='{'||c=='[') st.push(c);
    else if(c==')'||c=='}'||c==']'){
      if(st.empty()){
        cout<<"NO";
        return 0; }
      char t=st.top();
      st.pop();
      if((c==')'&&t!='(')||(c=='}'&&t!='{')||(c==']'&&t!='[')){
        cout<<"NO";
        return 0;
      }
    }
  }
cout<<(st.empty()?"YES":"NO");
return 0;
}
